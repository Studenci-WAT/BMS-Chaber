-    @Override
-    public Configuration addPackage(String packageName) throws MappingException {
-        List<Class<?>> classes = ClassFinder.find(packageName);
-        int size = classes.size();
-        for (int i = 0; i < size; i++) {
-            super.addAnnotatedClass(classes.get(i));
